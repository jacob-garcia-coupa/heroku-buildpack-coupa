#!/usr/bin/env bash
# bin/detect <BUILD_DIR>

set -eu -o pipefail

AWS_TOOLING_ACCESS_KEY_ID=$(cat $ENV_DIR/AWS_TOOLING_ACCESS_KEY_ID)
AWS_TOOLING_SECRET_ACCESS_KEY=$(cat $ENV_DIR/AWS_TOOLING_ACCESS_KEY_ID)

if [[ "$AWS_TOOLING_ACCESS_KEY_ID" == "" || "$AWS_TOOLING_SECRET_ACCESS_KEY}" == "" ]]
then
    echo "AWS credentials must exist in Heroku config to execute Coupa buildpack"
    exit 1
fi